{
  "name": "cs-dribbble-shots",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Desafio Concrete Front-end",
  "keywords": [
    "Desafio Concrete Front-end",
    "angular 2",
    "angular"
  ],
  "homepage": "http://rodrigopandini.com/cs-dribbble-shots",
  "author": {
    "name": "Rodrigo Pandini",
    "email": "rodrigopandini@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigopandini/cs-dribbble-shots.git"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start:staging": "ng serve --open --environment=staging",
    "start:prod": "ng serve --open --environment=prod",
    "build": "ng build --sourcemaps --output-hashing=bundles",
    "build:staging": "ng build --target=production --environment=staging --sourcemaps --output-hashing=bundles --no-progress",
    "build:prod": "ng build --target=production --environment=prod --output-hashing=bundles --no-progress",
    "lint": "ng lint --format stylish",
    "test": "ng test --single-run",
    "test:ci": "ng test --code-coverage --no-progress --browsers SL_CHROME,SL_FIREFOX,SL_SAFARI7,SL_EDGE,SL_ANDROID5 --single-run",
    "e2e": "ng e2e --no-watch",
    "e2e:ci": "ng e2e --no-progress --no-watch --target=production --environment=prod --browsers SL_CHROME,SL_ANDROID5",
    "cover": "ng test --watch=false --code-coverage",
    "compodoc": "./node_modules/.bin/compodoc -d docs -p src/tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jasmine-expect": "^3.6.0",
    "protractor-console-plugin": "^0.1.1",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0-beta.4",
    "compodoc": "0.0.41",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sauce-launcher": "^1.1.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0"
  }
}
